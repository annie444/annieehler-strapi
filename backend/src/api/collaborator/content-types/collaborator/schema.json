{
  "kind": "collectionType",
  "collectionName": "collaborators",
  "info": {
    "singularName": "collaborator",
    "pluralName": "collaborators",
    "displayName": "Collaborator"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "name",
      "required": true
    },
    "homepage": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "socialLinks": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "socials.social-links",
      "repeatable": true
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "inversedBy": "collaborators"
    }
  }
}
