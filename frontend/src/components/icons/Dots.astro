---
import type { SvgProps } from "@utils/svg";
import InternalSvg from "@components/icons/InternalSvg.astro";
type Props = SvgProps;
const { ...rest } = Astro.props as Props;

const height = 240;
const padX = 4;
const radius = 12;
const padY = 10;
const circles = 4;

const width = radius * 2 + padX * 2;
const diameter = radius * 2;

const cx = width / 2;

const circHeight = diameter * circles;
const circPad = padY * (circles - 1);
const totalCircHeight = circHeight + circPad;
const margin = (height - totalCircHeight) / 2;

const cy1 = margin + radius;
const cy2 = cy1 + padY + radius * 2;
const cy3 = cy2 + padY + radius * 2;
const cy4 = cy3 + padY + radius * 2;
---

<InternalSvg viewBox={{ x: 0, y: 0, width: width, height: height }} {...rest}>
  <circle cx={cx} cy={cy1} r={radius}></circle>
  <circle cx={cx} cy={cy2} r={radius}></circle>
  <circle cx={cx} cy={cy3} r={radius}></circle>
  <circle cx={cx} cy={cy4} r={radius}></circle>
</InternalSvg>
